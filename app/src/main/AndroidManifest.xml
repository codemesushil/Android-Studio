<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="shop.books.bookiesh">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".Walkthrough">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".RegistrationActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".BookActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity
            android:name=".Resetpassword"
            android:parentActivityName=".LoginActivity" />
        <activity
            android:name=".UploadImg"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".Mycart" />
        <activity android:name=".ConfirmRemove" />
        <activity android:name=".Address" />
        <activity android:name=".OrderSelectAddress" />
        <activity android:name=".Payment" />
        <activity android:name=".OrderConfirm"></activity>
    </application>

</manifest>